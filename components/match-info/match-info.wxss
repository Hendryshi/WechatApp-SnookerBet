.matchPart {
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  text-align: center;
}

.matchScore {
  height: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  line-height: 80rpx;
  font-size: 60rpx;
  font-weight: bold;
}

.matchStatus {
  display: flex;
  justify-content: center;
  line-height: 40rpx;
}

.matchType-none {
    display: none;
}
