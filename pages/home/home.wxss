.headBg {
  background-color:#5dbe8a;
  height: 400rpx;
  border-bottom-right-radius: 80rpx;
  border-bottom-left-radius: 80rpx;
  box-shadow: 4rpx 4rpx 8rpx rgba(0, 0, 0, 0.3);
}

/* 轮播图Go */
.title {
  width: 100%;
  height: 360rpx;
  padding-top: 10rpx;
}

.title .swip {
  box-shadow: 4rpx 4rpx 8rpx rgba(0, 0, 0, 0.3);
  margin: 0 auto;
  height: 280rpx;
  width: 650rpx;
  border-radius: 20rpx;
  overflow: hidden;
}

.title .swip image {
  width: 100%;
}
/* 轮播图Over */

.quizBox{
  margin: 20rpx;
  background: #eef7f2;
  height: 530rpx;
  line-height: 40rpx;
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
  border-radius: 30rpx;
  flex-direction: column;
}

.quizPart {
  height: 55%;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.quizBox image {
  width: 250rpx;
  margin: 20rpx;
  border-radius: 30rpx;
}

.quizInfo{
  width: 60%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 30rpx;
}

.btnPart{
  display: flex;
  align-items: center;
  text-align: center;
  flex-wrap: wrap;
  justify-content: center;
}

.btn{
  margin: 10rpx;
  width: 45%
}

.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.wrapper image{
  height: 800rpx;
}

.ruleBox{
  height: 800rpx;
  background-color: #f6f6f6;
}

.sheetContent{
  background-color: #f6f6f6;
  white-space:pre-wrap;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 200rpx;
}

.summaryBox{
  min-height: 400rpx;
  background-color: #f6f6f6;
}
