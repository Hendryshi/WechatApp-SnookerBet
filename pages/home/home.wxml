<view>
	<view class="top-title">
		{{gameName}}
	</view>
	<image class="top-bg" src="/images/background.jpg" mode="aspectFill" />
</view>

<view class="main-bg">
	<view wx:for="{{dataArr}}" wx:key="index" class="section">
		<view class="header" id="header" data-selectdata="{{item.Round}}">{{item.Round}}</view>
		<match-card
		 wx:for="{{item.AB_Bill}}"
		 wx:key="index"
		 catchtap="ClickCell"
		 data-id="{{item.AB_Money}}"
		>
			<player slot="player1" style="width:30%" showRank="True" />
			<player slot="player2" style="width:30%" showRank="True" />
			<match-info slot="matchInfo">
				
			</match-info>
		</match-card>
	</view>
</view>

